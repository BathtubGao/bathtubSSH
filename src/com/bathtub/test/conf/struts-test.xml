<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="test" extends="struts-default"
		namespace="/jsp/test">
		<action name="loginform">
			<result name="success">/WEB-INF/jsp/login.jsp</result>
		</action>

		<action name="login" class="com.bathtub.test.action.TestAction"
			method="userLogin">
			<result name="success">/WEB-INF/jsp/welcome.jsp</result>
			<result name="error">/WEB-INF/jsp/login.jsp</result>
		</action>

		<action name="insertUser">
			<result name="success">/WEB-INF/jsp/insertUser.jsp</result>
		</action>
		<action name="saveUser"
			class="com.bathtub.test.action.TestAction" method="save">
			<result name="success">/WEB-INF/jsp/welcome.jsp</result>
		</action>
	</package>
</struts>
